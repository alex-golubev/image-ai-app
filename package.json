{
  "name": "image-ai-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:playwright": "playwright test",
    "test:playwright-show-report": "playwright show-report",
    "drizzle:generate": "drizzle-kit generate:pg --config=src/db/config.ts",
    "drizzle:push": "drizzle-kit push:pg --config=src/db/config.ts",
    "drizzle:studio": "drizzle-kit studio --config=src/db/config.ts",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky; fi",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then playwright install; fi"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@tanstack/react-query": "^5.79.0",
    "@trpc/client": "^11.1.4",
    "@trpc/server": "^11.1.4",
    "@trpc/tanstack-react-query": "^11.1.4",
    "drizzle-orm": "^0.44.0",
    "drizzle-zod": "^0.8.2",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^2.2.2",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
